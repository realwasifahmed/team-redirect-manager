{
  "manifest_version": 3,
  "name": "Team Redirect Manager",
  "version": "1.1",
  "description": "Manage and redirect .test domains locally for your team.",
  "permissions": ["storage", "webNavigation", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
